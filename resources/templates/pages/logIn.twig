{% extends 'page.twig' %}
{% import 'form_fields.twig' as field %}
{% block content %}
	<form class="bs-form col-xs-12 col-md-6 col-md-offset-3" action="logIn.php" method="POST">
		{{ field.t({name:"email", type:"email", value:post_data.email}) }}
		{{ field.t({name:"password", type:"password"}) }}
		{{ field.submit("Log In") }}
	</form>
{% endblock %}